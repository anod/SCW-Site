
<ul class="thumbnails">
{% for skin in skins %}
<li class="span4">
  <div class="thumbnail">
    <img src="{{ skin.thumbSrc }}" alt="">
    <div class="caption">
      <h3>{{ skin.title }}</h3>
      <p><a href="{{ skin.downloadUrl }}" class="btn btn-primary"><i class="icon-download icon-white"></i>Download</a> 
      <a href="{{ skin.marketUrl }}" class="btn btn-success"><i class="icon-play icon-white"></i>Play Store</a></p>
    </div>
  </div>
</li>
{% endfor %}
</ul>